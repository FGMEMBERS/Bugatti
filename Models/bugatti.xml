  <effect>
    <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
    <object-name>fuselage</object-name>
    <object-name>sortie</object-name>
    <object-name>ailes</object-name>
    <object-name>aileronG</object-name>
    <object-name>aileronD</object-name>
    <object-name>voletGB</object-name>
    <object-name>voletDB</object-name>
    <object-name>voletGH</object-name>
    <object-name>voletDH</object-name>
    <object-name>derive</object-name>
    <object-name>direction</object-name>
    <object-name>profondeurG</object-name>
    <object-name>profondeurD</object-name>
    <object-name>porteG1</object-name>
    <object-name>porteG2</object-name>
    <object-name>porteG3</object-name>
    <object-name>porteD1</object-name>
    <object-name>porteD2</object-name>
    <object-name>porteD3</object-name>
    <object-name>pate</object-name>
    <object-name>bol1</object-name>
    <object-name>bol2</object-name>
  </effect>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitres</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitres</object-name>
    <object-name>propdisc1</object-name>
    <object-name>propblur1</object-name>
    <object-name>propdisc2</object-name>
    <object-name>propblur2</object-name>
  </animation>

  <!-- Une ombre pour OSG -->
  <model>
    <name>shadowOSG</name>
    <path>Aircraft/Bugatti/Models/shadow.xml</path>
  </model>

  <animation>
    <type>select</type>
    <object-name>shadowOSG</object-name>
    <condition>
      <equals>
        <property>sim[0]/rendering/shadows-ai</property>
        <value>true</value>
      </equals>
    </condition>
  </animation>

  <!-- Interieur -->
  <model>
    <path>Aircraft/Bugatti/Models/Interior/interior.xml</path>
    <offsets>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg>   0 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

  <!-- L'hélice intérieur--> 
  <animation>
    <name>HeliceI</name>
    <object-name>propdisc1</object-name>
    <object-name>propblur1</object-name>
    <object-name>helice1</object-name>
    <object-name>bol1</object-name>
  </animation>

  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc1</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur1</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[0]/rpm</property>
          <value>600.000</value>
        </greater-than>
        <less-than>
          <property>engines/engine[0]/rpm</property>
          <value>1500.001</value>
        </less-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>helice1</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceI</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
     <x-m> -3.478 </x-m>
     <y-m>  0.000 </y-m>
     <z-m>  0.182 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- L'hélice avant-->
  <animation>
    <name>HeliceE</name>
    <object-name>propdisc2</object-name>
    <object-name>propblur2</object-name>
    <object-name>helice2</object-name>
    <object-name>bol2</object-name>
  </animation>

  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc2</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur2</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[1]/rpm</property>
          <value>600.000</value>
        </greater-than>
        <less-than>
          <property>engines/engine[1]/rpm</property>
          <value>1500.001</value>
        </less-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>helice2</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/rpm</property>
        <value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceE</object-name>
    <property>engines/engine[1]/rpm</property>
    <factor> 1 </factor>
    <center>
     <x-m> -3.663 </x-m>
     <y-m>  0.000 </y-m>
     <z-m>  0.182 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeurG</object-name>
    <property>controls/flight/elevator</property>
    <factor> -20 </factor>
    <axis>
      <x1-m>  3.290 </x1-m>
      <y1-m> -0.121 </y1-m>
      <z1-m>  0.463 </z1-m>
      <x2-m>  3.290 </x2-m>
      <y2-m> -1.276 </y2-m>
      <z2-m>  1.040 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>profondeurD</object-name>
    <property>controls/flight/elevator</property>
    <factor> -20 </factor>
    <axis>
      <x1-m>  3.290 </x1-m>
      <y1-m>  1.276 </y1-m>
      <z1-m>  1.040 </z1-m>
      <x2-m>  3.290 </x2-m>
      <y2-m>  0.121 </y2-m>
      <z2-m>  0.463 </z2-m>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <property>controls/flight/rudder</property>
    <factor> 20 </factor>
    <axis>
      <x1-m>  3.351 </x1-m>
      <y1-m>  0.000 </y1-m>
      <z1-m> -0.270 </z1-m>
      <x2-m>  3.265 </x2-m>
      <y2-m>  0.000 </y2-m>
      <z2-m>  0.265 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>profondeurG</object-name>
    <property>controls/flight/rudder</property>
    <factor> 10 </factor>
    <axis>
      <x1-m>  3.290 </x1-m>
      <y1-m> -0.121 </y1-m>
      <z1-m>  0.463 </z1-m>
      <x2-m>  3.290 </x2-m>
      <y2-m> -1.276 </y2-m>
      <z2-m>  1.040 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>profondeurD</object-name>
    <property>controls/flight/rudder</property>
    <factor> -10 </factor>
    <axis>
      <x1-m>  3.290 </x1-m>
      <y1-m>  1.276 </y1-m>
      <z1-m>  1.040 </z1-m>
      <x2-m>  3.290 </x2-m>
      <y2-m>  0.121 </y2-m>
      <z2-m>  0.463 </z2-m>
    </axis>
  </animation>

